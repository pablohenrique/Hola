<IfModule mod_rewrite.c>
#    todas requisições para o site são redirecionadas para a pasta public
    RewriteEngine On
#    RewriteCond %{REQUEST_URI} !testes/
    RewriteCond %{REQUEST_URI} !public/
    RewriteRule ^(.*)$ public/$0 [L]
</IfModule>


